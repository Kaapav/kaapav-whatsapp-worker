{
  "projects": [
    {
      "src": "kaapav-chat-ui/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "kaapav-chat-ui/dist" }
    }
  ],
  "routes": [
    { "src": "/$", "dest": "/kaapav-chat-ui/index.html" },
    { "src": "/(.*)", "dest": "/kaapav-chat-ui/$1" }
  ]
}
